# import "..\typed_elements\table"
# import "..\named_elements\table"
# import "..\structural_features\table"
# import "table"

CREATE VIEW view_classes AS
SELECT 
	ne.id,
	ne.name,,
	r.abstraction
FROM
	named_elements ne,
	classifiers c,
	classes cs
WHERE
	ne.id = te.id 
	AND te.id = sf.id
	AND sf.id = r.id
;