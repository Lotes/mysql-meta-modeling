package de.loteslab.mmm.sqlc;

import java.io.File;
import java.util.HashSet;

public class Script {
	private File file;
	private String content;
	private HashSet<MySqlObject> imports = new HashSet<MySqlObject>();
	private HashSet<MySqlObject> exports = new HashSet<MySqlObject>();
	
	public Script(File file, String content) {
		this.file = file;
		this.content = content;
	}

	public HashSet<MySqlObject> getImports() {
		return imports;
	}

	public void setImports(HashSet<MySqlObject> imports) {
		this.imports = imports;
	}

	public HashSet<MySqlObject> getExports() {
		return exports;
	}

	public void setExports(HashSet<MySqlObject> exports) {
		this.exports = exports;
	}
}
