package de.loteslab.mmm.sqlc.lang;

import junit.framework.TestCase;

public class MySqlAnalyzerTest extends TestCase {
	private MySqlAnalyzer analyzer;
	
	@Override
	protected void setUp() throws Exception {
		analyzer = new MySqlAnalyzer();
	}
	
	public void testable() throws Exception {
		analyzer.scanRaw("CREATE TABLE mytable (id INT, PRIMARY KEY(id))");
		assertTrue(analyzer.getExports().contains(MySqlObject.create(MySqlObjectType.TABLE, "MYTABLE")));
	}
}
