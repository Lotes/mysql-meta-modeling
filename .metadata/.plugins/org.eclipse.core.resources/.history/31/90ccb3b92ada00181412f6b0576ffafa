#import "function_is_class"
#import "table_supertypes"
#import "table"

CREATE FUNCTION class_get_sub_types (
		given_class_id INT
	)
	RETURNS BOOL
	COMMENT 'returns all sub types in a temporary table `temp_sub_types`'
	LANGUAGE SQL
	NOT DETERMINISTIC
	MODIFIES SQL DATA
	SQL SECURITY DEFINER
BEGIN
	CREATE TEMPORARY TABLE temp_sub_types (sub_type_id INT PRIMARY KEY);	


	RETURN TRUE;
END;