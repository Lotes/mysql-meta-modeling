package de.loteslab.mmm.sqlc;

import java.io.File;
import java.util.HashSet;

public class Script {
	private File file;
	private String content;
	private HashSet<MySqlObject> imports = new HashSet<MySqlObject>();
	private HashSet<MySqlObject> exports = new HashSet<MySqlObject>();
	
	public Script(File file, String content) {
		this.setFile(file);
		this.setContent(content);
	}

	public HashSet<MySqlObject> getImports() {
		return imports;
	}

	public HashSet<MySqlObject> getExports() {
		return exports;
	}

	public File getFile() {
		return file;
	}

	public void setFile(File file) {
		this.file = file;
	}

	public String getContent() {
		return content;
	}

	public void setContent(String content) {
		this.content = content;
	}
}
